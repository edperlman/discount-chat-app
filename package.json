{
  "name": "discount-chat-app",
  "version": "7.2.0-develop",
  "description": "Rocket.Chat Monorepo",
  "main": "app.js",
  "private": true,
  "packageManager": "pnpm@9.15.1",
  "workspaces": [
    "apps/*",
    "packages/*",
    "ee/apps/*",
    "ee/packages/*",
    ".github/actions/*",
    "discount-chat-frontend"
  ],
  "scripts": {
    "dev": "turbo run dev --env-mode=loose --parallel",
    "build": "rimraf dist && turbo run build --filter=!root",
    "build:ci": "turbo run build:ci",
    "postinstall": "patch-package",
    "build:services": "turbo run build --filter=rocketchat-services...",
    "clean": "rimraf node_modules .turbo .pnpm-lock.yaml",
    "start": "node src/server.js",
    "test": "turbo run test",
    "testunit": "turbo run testunit",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{js,ts,json,md}\"",
    "storybook": "npm run storybook",
    "setup": "pnpm install && pnpm turbo run build",
    "fuselage": "./fuselage.sh",
    "fossify": "TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\"}' ts-node scripts/fossify.ts"
  },
  "dependencies": {
    "@types/stream-buffers": "^3.0.7",
    "dotenv": "^16.4.7",
    "mongodb": "^6.12.0",
    "node-gyp": "^11.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.9",
    "@types/chart.js": "^2.9.41",
    "@types/js-yaml": "^4.0.9",
    "balanced-match": "^1.0.2",
    "dotenv": "^16.4.7",
    "prettier": "^3.1.0",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "turbo": "^2.3.3",
    "patch-package": "^6.5.0",
    "postinstall-postinstall": "^2.1.0"
  },
  "pnpm": {
    "overrides": {
      "brace-expansion>balanced-match": "^1.0.2"
    }
  },
  "resolutions": {
    "brace-expansion/balanced-match": "^1.0.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RocketChat/Rocket.Chat.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/RocketChat/Rocket.Chat/issues"
  },
  "homepage": "https://github.com/RocketChat/Rocket.Chat#readme",
  "engines": {
    "node": "20.18.0",
    "npm": ">=8.0.0"
  },
  "peerDependencies": {
    "balanced-match": "^1.0.2"
  }
}
